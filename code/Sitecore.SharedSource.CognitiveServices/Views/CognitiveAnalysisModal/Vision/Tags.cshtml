@model Microsoft.ProjectOxford.Vision.Contract.Tag[]

@if (Model != null)
{
    <div>
        <h3>Tags</h3>
        @foreach (var t in Model)
        {
            <div>
                <div>
                    <label>@t.Name: </label>
                    <span>@t.Confidence.ToString("0.00") Confident</span>
                </div>
            </div>
        }
    </div>
}