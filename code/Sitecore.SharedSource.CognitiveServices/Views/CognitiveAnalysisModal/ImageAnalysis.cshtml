@model Sitecore.SharedSource.CognitiveServices.Models.ICognitiveImageAnalysis

@{
    ViewBag.Title = "Cognitive Services: Analysis";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/sitecore modules/Web/SitecoreCognitiveServices/Sitecore.SharedSource.CognitiveServices.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <h1>Image Editor</h1>
    <div>
        <h2>Emotion Analysis</h2>
        @foreach(var ea in Model.EmotionAnalysis)
        {
            @Html.Partial("Partials/Rectangle", ea.FaceRectangle)
            if(ea.Scores != null) { 
            <div>
                <h3>Scores</h3>
                <div>
                    <label>Anger: </label>
                    <span>@ea.Scores.Anger</span>
                </div>
                <div>
                    <label>Contempt: </label>
                    <span>@ea.Scores.Contempt</span>
                </div>
                <div>
                    <label>Disgust: </label>
                    <span>@ea.Scores.Disgust</span>
                </div>
                <div>
                    <label>Fear: </label>
                    <span>@ea.Scores.Fear</span>
                </div>
                <div>
                    <label>Happiness: </label>
                    <span>@ea.Scores.Happiness</span>
                </div>
                <div>
                    <label>Neutral: </label>
                    <span>@ea.Scores.Neutral</span>
                </div>
                <div>
                    <label>Sadness: </label>
                    <span>@ea.Scores.Sadness</span>
                </div>
                <div>
                    <label>Surprise: </label>
                    <span>@ea.Scores.Surprise</span>
                </div>
            </div>
            }
        }
    </div>
    <div>
        <h2>Facial Analysis</h2>
        @foreach (var fa in Model.FacialAnalysis)
        {
            if(fa.FaceAttributes != null) { 
            <div>
                <h3>Face Attributes</h3>
                <div>
                    <label>Age: </label>
                    <span>@fa.FaceAttributes.Age</span>
                </div>
                <div>
                    <label>FacialHair: </label>
                    <span>@fa.FaceAttributes.FacialHair</span>
                </div>
                <div>
                    <label>Gender: </label>
                    <span>@fa.FaceAttributes.Gender</span>
                </div>
                <div>
                    <label>Glasses: </label>
                    <span>@fa.FaceAttributes.Glasses</span>
                </div>
                <div>
                    <label>HeadPose: </label>
                    <span>@fa.FaceAttributes.HeadPose</span>
                </div>
                <div>
                    <label>Smile: </label>
                    <span>@fa.FaceAttributes.Smile</span>
                </div>
            </div>
            }
            if(fa.FaceId != null) {
            <div>
                <h3>Face Id:</h3>
                <div>
                    <span>@fa.FaceId</span>
                </div>
            </div>
            }
            if(fa.FaceLandmarks != null) { 
            <div>
                <h3>Face Landmarks</h3>
                <div>
                    <label>EyebrowLeftInner: </label>
                    <span>@fa.FaceLandmarks.EyebrowLeftInner</span>
                </div>
                <div>
                    <label>EyebrowLeftOuter: </label>
                    <span>@fa.FaceLandmarks.EyebrowLeftOuter</span>
                </div>
                <div>
                    <label>EyebrowRightInner: </label>
                    <span>@fa.FaceLandmarks.EyebrowRightInner</span>
                </div>
                <div>
                    <label>EyebrowRightOuter: </label>
                    <span>@fa.FaceLandmarks.EyebrowRightOuter</span>
                </div>
                <div>
                    <label>EyeLeftBottom: </label>
                    <span>@fa.FaceLandmarks.EyeLeftBottom</span>
                </div>
                <div>
                    <label>EyeLeftInner: </label>
                    <span>@fa.FaceLandmarks.EyeLeftInner</span>
                </div>
                <div>
                    <label>EyeLeftOuter: </label>
                    <span>@fa.FaceLandmarks.EyeLeftOuter</span>
                </div>
                <div>
                    <label>EyeLeftTop: </label>
                    <span>@fa.FaceLandmarks.EyeLeftTop</span>
                </div>
                <div>
                    <label>EyeRightBottom: </label>
                    <span>@fa.FaceLandmarks.EyeRightBottom</span>
                </div>
                <div>
                    <label>EyeRightInner: </label>
                    <span>@fa.FaceLandmarks.EyeRightInner</span>
                </div>
                <div>
                    <label>EyeRightOuter: </label>
                    <span>@fa.FaceLandmarks.EyeRightOuter</span>
                </div>
                <div>
                    <label>EyeRightTop: </label>
                    <span>@fa.FaceLandmarks.EyeRightTop</span>
                </div>
                <div>
                    <label>MouthLeft: </label>
                    <span>@fa.FaceLandmarks.MouthLeft</span>
                </div>
                <div>
                    <label>MouthRight: </label>
                    <span>@fa.FaceLandmarks.MouthRight</span>
                </div>
                <div>
                    <label>NoseLeftAlarOutTip: </label>
                    <span>@fa.FaceLandmarks.NoseLeftAlarOutTip</span>
                </div>
                <div>
                    <label>NoseLeftAlarTop: </label>
                    <span>@fa.FaceLandmarks.NoseLeftAlarTop</span>
                </div>
                <div>
                    <label>NoseRightAlarOutTip: </label>
                    <span>@fa.FaceLandmarks.NoseRightAlarOutTip</span>
                </div>
                <div>
                    <label>NoseRightAlarTop: </label>
                    <span>@fa.FaceLandmarks.NoseRightAlarTop</span>
                </div>
                <div>
                    <label>NoseRootLeft: </label>
                    <span>@fa.FaceLandmarks.NoseRootLeft</span>
                </div>
                <div>
                    <label>NoseRootRight: </label>
                    <span>@fa.FaceLandmarks.NoseRootRight</span>
                </div>
                <div>
                    <label>NoseTip: </label>
                    <span>@fa.FaceLandmarks.NoseTip</span>
                </div>
                <div>
                    <label>PupilLeft: </label>
                    <span>@fa.FaceLandmarks.PupilLeft</span>
                </div>
                <div>
                    <label>PupilRight: </label>
                    <span>@fa.FaceLandmarks.PupilRight</span>
                </div>
                <div>
                    <label>UnderLipBottom: </label>
                    <span>@fa.FaceLandmarks.UnderLipBottom</span>
                </div>
                <div>
                    <label>UnderLipTop: </label>
                    <span>@fa.FaceLandmarks.UnderLipTop</span>
                </div>
                <div>
                    <label>UpperLipBottom: </label>
                    <span>@fa.FaceLandmarks.UpperLipBottom</span>
                </div>
                <div>
                    <label>UpperLipTop: </label>
                    <span>@fa.FaceLandmarks.UpperLipTop</span>
                </div>
            </div>
            }
            @Html.Partial("Partials/FaceRectangle", fa.FaceRectangle)
        }
    </div>
    <div>
        <h2>Text Analysis</h2>
        <div>
            <label>Language: </label>
            <span>@Model.TextAnalysis.Language</span>
        </div>
        <div>
            <label>Orientation: </label>
            <span>@Model.TextAnalysis.Orientation</span>
        </div>
        @if(Model.TextAnalysis.Regions != null) { 
            foreach (var r in Model.TextAnalysis.Regions)
            {
                <div>
                    <h3>Regions</h3>
                    @if(r.BoundingBox != null) {
                    <div>
                        <label>Bounding Box: </label>
                        <span>@r.BoundingBox</span>
                    </div>
                    }
                    @if (r.Lines != null)
                    {
                    <div>
                        <h4>Lines: </h4>
                        @foreach (var l in r.Lines)
                        {
                        <div>
                            <label></label>
                            <span>@l.BoundingBox</span>
                        </div>
                        @Html.Partial("Partials/Rectangle", l.Rectangle)
                        @*<div>
                            <label></label>
                            <span>@l.Words</span>
                        </div>*@
                        }
                    </div>
                    }
                    @Html.Partial("Partials/Rectangle", r.Rectangle)
                </div>
            }
        }
        @if (Model.TextAnalysis.TextAngle != null) {
        <div>
            <label>Text Angle: </label>
            <span>@Model.TextAnalysis.TextAngle</span>
        </div>
        }
    </div>
    <div>
        <h2>Vision Analysis</h2>
        @if (Model.VisionAnalysis.Adult != null)
        {
        <div>
            <h3>Adult</h3>
            <div>
                <label></label>
                <span>@Model.VisionAnalysis.Adult.AdultScore</span>
            </div>
            <div>
                <label></label>
                <span>@Model.VisionAnalysis.Adult.IsAdultContent</span>
            </div>
            <div>
                <label></label>
                <span>@Model.VisionAnalysis.Adult.IsRacyContent</span>
            </div>
            <div>
                <label></label>
                <span>@Model.VisionAnalysis.Adult.RacyScore</span>
            </div>
        </div>
        }
        @foreach(var c in Model.VisionAnalysis.Categories)
        {

        }
        @if(Model.VisionAnalysis.Color != null) {
        <div>
            <h3>Color</h3>
            <div>
                <label>AccentColor: </label>
                <span>@Model.VisionAnalysis.Color.AccentColor</span>
            </div>
            <div>
                <label>DominantColorBackground: </label>
                <span>@Model.VisionAnalysis.Color.DominantColorBackground</span>
            </div>
            <div>
                <label>DominantColorForeground: </label>
                <span>@Model.VisionAnalysis.Color.DominantColorForeground</span>
            </div>
            <div>
                <h4>Dominant Colors</h4>
                @foreach (var c2 in @Model.VisionAnalysis.Color.DominantColors)
                {
                    <div>
                        <span>@c2</span>
                    </div>
                }
            </div>
            <div>
                <label>Is Black White Image: </label>
                <span>@Model.VisionAnalysis.Color.IsBWImg</span>
            </div>
        </div>
        }
        @if(Model.VisionAnalysis.Description != null) {
            <h3>Description</h3>
            <div>
                <h4>Captions</h4>
                @foreach (var cp in Model.VisionAnalysis.Description.Captions)
                {
                    <div>
                        <label>Confidence: </label>
                        <span>@cp.Confidence</span>
                    </div>
                    <div>
                        <label>Text: </label>
                        <span>@cp.Text</span>
                    </div>
                }
            </div>
            <div>
                <h4>Tags</h4>
                @foreach (var t in Model.VisionAnalysis.Description.Tags)
                {
                    <div>
                        <span>@t</span>
                    </div>
                }
            </div>
        }
        @*@Model.VisionAnalysis.Faces
        @Model.VisionAnalysis.ImageType
        @Model.VisionAnalysis.Metadata
        @Model.VisionAnalysis.RequestId
        @Model.VisionAnalysis.Tags
        <div>
            <label></label>
            <span></span>
        </div>*@
    </div>
    <script src="~/sitecore modules/Web/SitecoreCognitiveServices/jquery-1.10.2.min.js"></script>
</body>
</html>