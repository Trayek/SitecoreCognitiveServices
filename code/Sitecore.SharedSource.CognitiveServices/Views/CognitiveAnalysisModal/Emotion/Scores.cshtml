@model Microsoft.ProjectOxford.Emotion.Contract.Scores

@if(Model != null) {
    <div class="bg-layer"></div>
    <div class="score-set">
        <label>Anger: </label>
        <span>@Model.Anger.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Contempt: </label>
        <span>@Model.Contempt.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Disgust: </label>
        <span>@Model.Disgust.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Fear: </label>
        <span>@Model.Fear.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Happiness: </label>
        <span>@Model.Happiness.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Neutral: </label>
        <span>@Model.Neutral.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Sadness: </label>
        <span>@Model.Sadness.ToString("0.00")</span>
    </div>
    <div class="score-set">
        <label>Surprise: </label>
        <span>@Model.Surprise.ToString("0.00")</span>
    </div>
}