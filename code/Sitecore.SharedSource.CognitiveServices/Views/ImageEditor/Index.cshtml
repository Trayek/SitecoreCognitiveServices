@using Sitecore.SharedSource.CognitiveServices.Models
@model ICognitiveImage
@{
    ViewBag.Title = "Cognitive Services: Image Editor";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/sitecore modules/Web/SitecoreCognitiveServices/Sitecore.SharedSource.CognitiveServices.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <h1>Image Editor</h1>
    <div>@Model.Analysis</div>
<form
    method="post"
    action="/sccogsvcs/ImageEditor/Analyze">
    <input type="hidden" name="itemId" value="@Model.ItemId"/>
    <input type="hidden" name="database" value="@Model.Database" />
    <input type="hidden" name="language" value="@Model.Language" />
    <button>Analyze</button>
</form>
    <script src="~/sitecore modules/Web/SitecoreCognitiveServices/jquery-1.10.2.min.js"></script>
</body>
</html>